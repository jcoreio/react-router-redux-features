export type Options<S, A> = {
  route: PlainRoute | (store: Store<S, A>) => PlainRoute,
  store: Store<S, A>,
  featureId: string,
  featureName: string,
  isServer?: boolean,
  getFeatureStates: (state: S) => FeatureStates,
  rematchRoutes?: (store: Store<S, A>) => any,
